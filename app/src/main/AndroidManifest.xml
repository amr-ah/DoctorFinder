<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="capstone.com.doctorfinder">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/doctorfinder"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/doctorfinder"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity"/>
        <activity android:name=".SignUpActivity"/>
        <activity android:name=".DoctorSignup"/>
        <activity android:name=".DoctorMenu"/>
        <activity android:name=".search"/>
        <activity android:name=".DoctorProfile">
        </activity>
    </application>

</manifest>